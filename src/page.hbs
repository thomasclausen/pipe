{{!< default}}

{{#post}}
	{{#contentFor "meta-facebook"}}
		<meta property="og:type" content="article" />
	{{/contentFor}}

	<article id="page-{{id}}" class="{{post_class}}" role="article">
		<div class="page-content">
			<h1 class="page-title">{{{title}}}</h1>
			{{content}}
		</div>
	</article>
{{/post}}
